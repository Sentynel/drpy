set view map
unset surface
set style data pm3d
set style function pm3d
#set pm3d map
set pm3d implicit at b
set pm3d flush begin noftriangles scansforward
set xrange [1967 : 2014]
set yrange [5 : 16]
set ticslevel 0
set xlabel "Year"
set ylabel "DR"
set title "DR count by year"
print "Load this and type splot \"Filename\""
